<% form_for @sitesetting do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :support_email %><br />
    <%= f.text_field :support_email %>
  </p>
  <p>
    <%= f.label :um_staff_email %><br />
    <%= f.text_field :um_staff_email %>
  </p>
  <p>
    <%= f.label :um_paypal %><br />
    <%= f.text_field :um_paypal %>
  </p>
  <p>
    <%= f.label :sales_percent %><br />
    <%= f.text_field :sales_percent %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
